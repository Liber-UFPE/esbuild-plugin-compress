{
  "name": "@liber-ufpe/esbuild-plugin-compress",
  "engines": {
    "node": ">=18"
  },
  "version": "1.1.0",
  "description": "esbuild plugin to compress output using gzip, brotli, and deflate.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Liber-UFPE/esbuild-plugin-compress.git"
  },
  "scripts": {
    "build": "tsc",
    "prepack": "npm run build"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/*.*"
  ],
  "keywords": [
    "esbuild",
    "compress",
    "brotli",
    "gzip",
    "deflate",
    "plugin"
  ],
  "author": {
    "name": "Liber UFPE",
    "url": "https://github.com/Liber-UFPE/"
  },
  "maintainers": [
    {
      "name": "Marcos Pereira",
      "url": "https://github.com/marcospereira/"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Liber-UFPE/esbuild-plugin-compress/issues"
  },
  "homepage": "https://github.com/Liber-UFPE/esbuild-plugin-compress#readme",
  "dependencies": {
    "esbuild": "^0.23.0",
    "picomatch": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.22",
    "@types/picomatch": "^3.0.0",
    "typescript": "^5.3.3"
  }
}
